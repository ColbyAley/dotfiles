[user]
  name = Colby Aley
  email = colby@aley.me
[color]
  ui = true
[alias]
  pop = reset --soft HEAD~1
  ci = commit
  co = checkout
  br = branch
  st = status
  a = add
  d = rm
  sh = show
  pr = !git push origin $(git rev-parse --abbrev-ref HEAD) && open http://github.com/cloudability/$(basename $PWD)/compare/master...$(git rev-parse --abbrev-ref HEAD)
[core]
  excludesfile = ~/.gitignore
[init]
	templatedir = ~/.git-templates
